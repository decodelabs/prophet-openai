{
    "name": "decodelabs/prophet-openai",
    "description": "Open AI API adapter for the DecodeLabs Prophet assistant system",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.1",
        "decodelabs/exceptional": "^0.4.5",
        "decodelabs/prophet": "^0.1.0",

        "openai-php/client": "^0.8.5",
        "nesbot/carbon": "^2|^3"
    },
    "require-dev": {
        "decodelabs/glitch": "^0.18.11",
        "decodelabs/phpstan-decodelabs": "^0.6.7",

        "guzzlehttp/guzzle": "^7.8.1",
        "guzzlehttp/psr7": "^2.6.2"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Prophet\\Platform\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
