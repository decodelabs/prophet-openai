{
    "name": "decodelabs/prophet-openai",
    "description": "Open AI API adapter for the DecodeLabs Prophet assistant system",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/prophet": "^0.5.1",

        "openai-php/client": "^0.15",
        "nesbot/carbon": "^3"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7",

        "guzzlehttp/guzzle": "^7.9.3",
        "guzzlehttp/psr7": "^2.7.1"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Prophet\\Platform\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
    }
}
